      "targets": [
        {
          "alias": "",
          "datasource": {
            "type": "mssql",
            "uid": "${DS_MICROSOFT_SQL SERVER}"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH TotalCounts AS (\r\n    SELECT COUNT(alarmID) AS TotalAlarms\r\n    FROM LogRhythm_Alarms.dbo.Alarm a WITH (NOLOCK)\r\n    JOIN LogRhythmEMDB.dbo.AlarmRule ar ON a.AlarmRuleID = ar.AlarmRuleID\r\n    WHERE DateInserted BETWEEN DATEADD(DAY, -7, GETDATE()) AND GETDATE()\r\n)\r\nSELECT \r\n    CASE \r\n        WHEN AlarmStatus = 0 THEN 'New'\r\n        WHEN AlarmStatus = 1 THEN 'OpenAlarm'\r\n        WHEN AlarmStatus = 2 THEN 'Working'\r\n        WHEN AlarmStatus = 3 THEN 'Escalated'\r\n        WHEN AlarmStatus = 4 THEN 'AutoClosed'\r\n        WHEN AlarmStatus = 5 THEN 'FalsePositive'\r\n        WHEN AlarmStatus = 6 THEN 'Resolved'\r\n        WHEN AlarmStatus = 7 THEN 'UnResolved'\r\n        WHEN AlarmStatus = 8 THEN 'Reported'\r\n        WHEN AlarmStatus = 9 THEN 'Monitor'\r\n    END AS AlarmStatus,\r\n    CONVERT(decimal(5,2), COUNT(a.alarmID) * 100.0 / (SELECT TotalAlarms FROM TotalCounts)) AS Percentage\r\nFROM LogRhythm_Alarms.dbo.Alarm a WITH (NOLOCK)\r\nJOIN LogRhythmEMDB.dbo.AlarmRule ar ON a.AlarmRuleID = ar.AlarmRuleID\r\nWHERE $__timeFilter(DateInserted)\r\nGROUP BY AlarmStatus\r\nORDER BY Percentage DESC;\r\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
