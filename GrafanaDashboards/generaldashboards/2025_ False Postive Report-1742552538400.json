      "targets": [
        {
          "alias": "",
          "datasource": {
            "type": "mssql",
            "uid": "${DS_MICROSOFT_SQL SERVER}"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n    Priority,\n    COUNT(*) AS Count\nFROM (\n    -- Subquery to join Alarm and MARCMsg tables\n    SELECT \n        alm.AlarmDate, \n        alm.AlarmStatus, \n        atm.MARCMsgID \n    FROM [LogRhythm_Alarms].[dbo].[Alarm] alm\n    JOIN [LogRhythm_Alarms].[dbo].[AlarmToMARCMsg] atm \n        ON alm.AlarmID = atm.AlarmID\n) almm\nJOIN (\n    -- Subquery to categorize priority levels\n    SELECT \n        MsgID,\n        CASE \n            WHEN Priority >= 90 THEN 'Critical' \n            WHEN Priority BETWEEN 75 AND 89 THEN 'Very High' \n            WHEN Priority BETWEEN 65 AND 74 THEN 'High' \n            WHEN Priority BETWEEN 50 AND 64 THEN 'Medium' \n            ELSE 'Low' \n        END AS Priority \n    FROM LogRhythm_Events.dbo.Msg\n) emsg \nON emsg.MsgID = almm.MARCMsgID\nWHERE \n    $__timeFilter(almm.AlarmDate)  -- Dynamic time filter\n    AND almm.AlarmStatus = 5  -- Only FalsePositive alarms\nGROUP BY Priority\nORDER BY \n    CASE Priority\n        WHEN 'Critical' THEN 1\n        WHEN 'Very High' THEN 2\n        WHEN 'High' THEN 3\n        WHEN 'Medium' THEN 4\n        WHEN 'Low' THEN 5\n    END;\n",
          "refId": "A",
